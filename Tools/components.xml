<components>
  <transform>
    <pos type="Vector3" />
    <angle type="float" />
  </transform>
  <polygon>
    <points type="List{Vector3}" />
    <vectLine type="VectorLine" />
    <isDirty type="bool" />
    <color type="Color" />
    <lineWidth type="float" />
  </polygon>
  <playerScore>
    <score type="int" />
  </playerScore>
  <bulletTag />
  <shipTag />
  <asteroidTag />
  <teamId>
    <index type="int" />
  </teamId>
  <drawColor>
    <color type="Color"/>
  </drawColor>
  <asteroidSize>
    <radius type="float"/>
  </asteroidSize>
  <keyBindings>
    <keys type="List{KeyBinding}" />
  </keyBindings>
  <velocity>
    <vel type="Vector3" />
  </velocity>
  <inputBuffer>
    <keys type="List{KeyState}" />
    <steer type="float" />
    <throttle type="float" />
    <isShooting type="bool" />
    <shootCooldown type="float" />
    <maxCooldown type="float" />
  </inputBuffer>
  <screenLimits>
    <width type="float" />
  </screenLimits>
  <pointTag/>
  <inputDrivesThisEntityTempTag/>
  <collisionDisk>
    <radius type="float" />
  </collisionDisk>
  <entityLifetime>
    <secondsRemaining type="float" />
  </entityLifetime>
  <playerData>
    <score type="int" />
    <lives type="int" />
    <waveIndex type="int" />
  </playerData>
  <gameState>
    <currentState type="ECSWorld.ECSteroidsGameState" />
  </gameState>
  <pendingGameState>
    <nextState type="ECSWorld.ECSteroidsGameState" />
    <secondsRemaining type="float" />
  </pendingGameState>
  <textMessage>
    <text type="string" />
    <pos type="Vector3" />
    <scale type="float" />
    <color type="Color" />
    <width type="float" />
    <vectLine type="VectorLine" />
  </textMessage>
  <bootStateTag />
  <titleStateTag />
  <gameplayStateTag />
  <temporaryInvulnerability>
    <secondsRemaining type="float" />
  </temporaryInvulnerability>
  <levelDesc>
    <index type="int" />
    <name type="string" />
    <numAsteroids type="int" />
    <numStaticCoins type="int" />
    <timeLimitSeconds type="int" />
    <numDynamicCoins type="int" />
    <numSaucersBig type="int" />
    <numSaucersSmall type="int" />
  </levelDesc>
</components>
